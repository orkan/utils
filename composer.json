{
	"name": "orkan/utils",
	"description": "Helper functions",
	"homepage": "https://github.com/orkan/utils",
	"keywords": [
		"php",
		"ant",
		"git",
		"bat"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Orkan",
			"role": "Lead",
			"email": "orkans+utils@gmail.com"
		}
	],
	"config": {
		"vendor-dir": "../vendor"
	},
	"require": {
		"php": "^7.4",
		"composer-runtime-api": "^2.2"
	},
	"require-dev": {
		"monolog/monolog": "^2.9",
		"phpunit/phpunit": "^9.6"
	},
	"autoload": {
		"psr-4": {
			"Orkan\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Orkan\\Tests\\": "tests/"
		}
	},
	"bin": [
		"ork-env-switch",
		"ork-files-sync"
	],
	"extra": {
		"branch-alias": {
			"dev-master": "11.3.x-dev",
			"dev-develop": "11.x-dev"
		}
	}
}
