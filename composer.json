{
	"name": "orkan/utils",
	"description": "Helper functions",
	"homepage": "https://github.com/orkan/utils",
	"keywords": [
		"php"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Orkan",
			"role": "Lead",
